@inproceedings{beck2015mevolte,
author = {Michael Till Beck and Sebastian Feld and Andreas Fichtner and Claudia Linnhoff-Popien and Thomas Schimper},
title = {ME-VoLTE: Energy-Efficient Video Transcoding At the Mobile Edge},
booktitle = {18th International ICIN Conference, Innovations in Services, Networks, and Clouds (ICIN 2015)},
