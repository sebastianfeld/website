@inproceedings{feld2010security,
author = {Sebastian Feld and Norbert Pohlmann},
title = {Security Analysis of OpenID, followed by a Reference Implementation of an nPA-based OpenID Provider},
booktitle = {ISSE 2010 Securing Electronic Business Processes},
year = {2010},
